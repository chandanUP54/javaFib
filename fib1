public class fibonacciRecursion {
	  public static int fibRecursion(int count) {
	    if (count == 0) 
	      return 0;
	    if (count == 1 || count == 2) 
	      return 1;
  return fibRecursion(count - 2) + fibRecursion(count - 1); //function calling itself
	  }
	  public static void main(String args[]) {
	    int fib_len = 9;
	    System.out.println("Fibonacci Series of is:");
	    for (int i = 0; i < fib_len; i++) {
	      System.out.println(fibRecursion(i)); 
	    }
	  }
	}
